package pewarisan;

    public class Latihan_6 {
        
        static int tambah(int a, int b) {
            return a + b;
        }
        
        static double tambah(double a, double b) {
            return a + b;
        }
        
        static int tambah(int a, int b, int c) {
            return a + b + c;
        }
        
        public static void main(String[] args) {
            System.out.println("hasil : " + tambah(1, 2));
            System.out.println("hasil : " + tambah(0.6, 8.6));
            System.out.println("hasil : " + tambah(2, 11, 54));
        }
}
